generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("MONGODB_URI")
}

model DriveFile {
  id          String  @id @default(auto()) @map("_id") @db.ObjectId
  name        String
  fileName    String
  mimeType    String
  description String?
  userId      String
}

model Project {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  name        String
  description String?
  images      String[]
  category    String
  link        String?
}
